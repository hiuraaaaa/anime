{
  "headers": [
    {
      "source": "/(.*)\\.m3u8",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.apple.mpegurl"
        }
      ]
    },
    {
      "source": "/(.*)\\.ts",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp2t"
        }
      ]
    }
  ]
}